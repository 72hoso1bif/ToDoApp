{
  "name": "todolist",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "ng serve --proxy-config proxy.conf.json",
    "start": "node server.js",
    "build": "ng build --prod",
    "test-ci": "ng test --sourceMap=false --browsers=ChromeHeadless --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev",
    "webapp:build:dev": "ng build",
    "ci:backend:prepare": "./mvnw spring-boot:start",
    "ci:backend:teardown": "./mvnw spring-boot:stop",
    "ci:backend:test": "npm run ci:backend:prepare && ./mvnw test && npm run ci:backend:teardown",
    "ci:frontend:build": "npm run webapp:build:dev",
    "ci:frontend:test": "npm run ci:frontend:build && npm run test-ci"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "lazysizes": "^5.3.2",
    "ngx-icon-picker": "^1.7.0",
    "rxjs": "~6.4.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular/cli": "^8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "ngx-spec": "^2.0.0",
    "protractor": "^7.0.0",
    "puppeteer": "^10.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
